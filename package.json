{
  "name": "ayubo-lanka",
  "version": "1.0.0",
  "description": "This is our 2nd year 2nd semester project",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "backend": "nodemon backend/server.js",
    "frontend": "cd frontend/vite-project && vite --host",
    "dev": "concurrently \"npm run backend\" \"npm run frontend\"",
    "build": "cd frontend/vite-project && vite build",
    "start": "NODE_ENV=production node backend/server.js",
    "install-client": "cd frontend/vite-project && npm install",
    "install-server": "npm install",
    "install-all": "npm run install-server && npm run install-client"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MalithSamarakoon/Ayubo-Lanka.git"
  },
